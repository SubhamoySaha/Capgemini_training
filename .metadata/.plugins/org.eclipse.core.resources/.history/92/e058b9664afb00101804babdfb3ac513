package Tree_Set;


public class Employee implements Comparable<Employee> {
	
	int id;
	String name;
	double Salary;
	
	Employee(int id,String name,double Salary){
		this.id=id;
		this.name=name;
		this.Salary=Salary;
	}
	
	public int compareTo(Employee e) {
		if(this.Salary>e.Salary) {
			return 1;
		}
		if(this.Salary<e.Salary) {
			return -1;
		}
		if(this.Salary==e.Salary) {
			if(this.name.compareTo(e.name)>0) {
				return 1;
			}
			if(this.name.compareTo(e.name)<0) {
				return -1;
			}
			if(this.name.compareTo(e.name)==0) {
				if(this.id>e.id) {
					return 1;
				}
				if(this.id<e.id) {
					return -1;
				}
			}
		}
		return 0;
	}
	
	@Override
	public String toString() {
		return "\n"+this.id+","+this.name+","+this.Salary;
	}
	
	
}

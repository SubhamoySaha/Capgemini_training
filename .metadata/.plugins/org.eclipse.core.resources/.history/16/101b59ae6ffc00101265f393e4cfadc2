package Online_Banking_Transaction_System;

public class Main {

	public static void main(String[] args) {
		
		Account p1=new Savings(2441129,"Henry");
		try {
		p1.Deposit(2000);
		p1.Deposit(4000);
		}
		
		catch(InsufficientBalanceException e){
			System.out.println(e.getMessage());
		}
		
		try {
			p1.Withdraw(500);
		}
		
		catch(InsufficientBalanceException e){
			System.out.println(e.getMessage());
		}
		
		finally{
			p1.getDetails();
		}
		
		
	}
}

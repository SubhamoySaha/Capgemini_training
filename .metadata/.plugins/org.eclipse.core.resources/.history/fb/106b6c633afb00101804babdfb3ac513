package hashcode;

public class Person {

	int id;
	String email;
	
	
	
	public Person(int id,String email) {
		this.id=id;
		this.email=email;
		}
	
	public boolean equals(Object o) {
		Person p=(Person)o;
		if(this.id==p.id && this.email==p.email) {
			return true;
		}
		return false;
	}
	
	public class Main {

	    public static void main(String[] args) {
	        Person p1 = new Person(10, "abc@gamil.com");
	        Person p2 = new Person(10, "abc@gmail.com");
	        if (p1.equals(p2)) {
	            System.out.println("Equal ");
	        } else {
	            System.out.println("Not Equal ");
	        }
	    }
	
}

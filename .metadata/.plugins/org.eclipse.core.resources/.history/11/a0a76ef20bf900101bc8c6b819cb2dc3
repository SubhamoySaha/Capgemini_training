package Test_24_1_26;

abstract class AbstractC {
    AbstractC() {
        System.out.print("Abstract ");
    }
    abstract void show();
}

class ConcreteC extends AbstractC {
    ConcreteC() {
        System.out.print("Concrete ");
    }
    void show() {
        System.out.print("Show");
    }
}


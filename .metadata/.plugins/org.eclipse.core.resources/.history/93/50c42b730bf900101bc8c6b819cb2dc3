package Test_24_1_26;

public class Test3 {
	
	 public static void main(String[] args) {
	        int x = 10;
	        change(x);
	        System.out.println(x);
	    }

	    static void change(int x) {
	        x = x + 5;
	        modify(x);
	    }

	    static void modify(int x) {
	        x = x * 2;
	    }

}

package LinkedHashSet;

import java.util.*;

public class LinkedHashSetDemo {

	public static void main(String[] args) {
		
		LinkedHashSet<Integer> lhs=new LinkedHashSet<Integer>();
		lhs.add(100);
		lhs.add(10);
		lhs.add(50);
		lhs.add(70);
		
		System.out.println(lhs);
		
		//itereate here using for each, Iterator, Listiterator
		
		
		
		for(int i:lhs) {
			System.out.println(i);
		}
		
		Iterator<Integer> itr=lhs.iterator();
		while(itr.hasNext()) {
			System.out.println(itr.next());
		}
		
		//listIterator is not possible asit is only applicable in list
		
//		ListIterator<Integer> itr=lhs.listIterator();
//		while(itr.hasNext()) {
//			System.out.println(itr.next());
//		}
		
		int[] a= {1,2,3,4};
		//ArrayList<Integer> list1=new ArrayList<Integer>(a);
		ArrayList<int[]> list1=new ArrayList<int[]>(Arrays.asList(a));
		for(int[] i:list1) {
		System.out.println(i);}
		
		
		
		ArrayList<Integer> list=new ArrayList<Integer>(lhs);
		ListIterator<Integer> itr2=list.listIterator();
		while(itr.hasNext()) {
		System.out.println(itr.next());
		}
	}
}
